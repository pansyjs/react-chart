{
  "name": "@pansy/react-charts",
  "version": "1.0.0",
  "description": "react chart",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "repository": "git@github.com:pansyjs/react-charts.git",
  "author": "Pansy Team",
  "license": "MIT",
  "scripts": {
