    "release": "walrus release"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@antv/g2plot": "^2.0.4",
    "@antv/util": "^2.0.9",
    "react-content-loader": "^5.1.3",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@walrus/cli": "^1.3.4",
    "@walrus/plugin-entry": "^0.2.0",
    "@walrus/plugin-release": "^1.9.1",
    "@walrus/preset-lint": "^1.1.7",
    "@walrus/test": "^1.1.3",
    "dumi": "^1.0.34",
    "father-build": "^1.18.5",
    "husky": "3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier",
